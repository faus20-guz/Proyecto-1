    private void loginIngresarButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                    
        adminPage newPage = new adminPage();
        if(loginEmailField.getText().equals(usuarioAdmin)&&loginPasswordField.getText().equals(passAdmin)){
            this.setVisible(false);
            newPage.setVisible(true);
        }

    }                                                   

    private void loginCrearCuentaButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                       
        // TODO add your handling code here:
        registroUsuarios nuevoUsuario = new registroUsuarios();
        nuevoUsuario.setVisible(true);
        this.setVisible(false);
    }                                                      

